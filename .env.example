# Configuración de Supabase
# URL de tu proyecto de Supabase
SUPABASE_URL=your_supabase_project_url

# Clave de servicio de Supabase (Service Role Key)
# IMPORTANTE: Esta clave tiene permisos completos, no la compartas públicamente
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Configuración del servicio de correo electrónico
# Email desde el cual se enviarán los correos
Email=your_email@gmail.com

# Contraseña del email o contraseña de aplicación (App Password)
# Para Gmail, necesitas generar una contraseña de aplicación desde tu cuenta de Google
Password=your_email_password_or_app_password

# URL web (opcional)
# URL base de tu aplicación web, utilizada en las plantillas de email
URL_WEB=https://your-website.com
